{
  "name": "seer-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "^3.2.10",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "date-fns": "^2.16.1",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3",
    "serve": "^11.3.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "husky": "^4.3.0"
  },
  "scripts": {
    "start": "serve -s build",
    "startDev": "react-scripts start",
    "build": "react-scripts build",
    "dockerStart": "docker build -t static-server . && docker run --name ReactNginx --rm -d -p 80:80 static-server",
    "dockerClean": "docker stop $(docker container ls -q --filter name=ReactNginx)",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint -f table \"./src/**/*.js\""
  },
  "homepage": ".",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && CI=true npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
